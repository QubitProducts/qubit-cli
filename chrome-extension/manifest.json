{
  "name": "xp-cli",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "xp-cli chrome extension",
  "browser_action": {},
  "icons": {
    "16": "icons/on16.png",
    "48": "icons/on48.png",
    "128": "icons/on128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["index.js"]
  }],
  "permissions": [
    "storage",
    "cookies",
    "https://app.qubit.com/*",
    "https://staging-dashboard.qubitproducts.com/*",
    "http://localhost:3000/*"
  ]
}
