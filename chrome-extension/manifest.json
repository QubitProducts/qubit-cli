{
  "name": "qubit-cli",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Qubit-CLI chrome extension",
  "browser_action": {},
  "icons": {
    "16": "icons/on16.png",
    "48": "icons/on48.png",
    "128": "icons/on128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "storage",
    "cookies",
    "tabs",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
  ]
}
